public class CheckPallindrome {

    public static void main(String[] args) {
        int rev=0;
        int num=Integer.parseInt(args[0]);
        for(int i=num;i!=0;i/=10){
            rev=rev*10+i%10;
        }
        if(rev==num){
            System.out.println(""+num+"is a pallindrome");
        }
        else{
            System.out.println(""+num+"not a pallindrome");
        }
    }
}
